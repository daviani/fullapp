{
  "builds": [
    {
      "src": "apps/frontend",
      "use": "npx nx build frontend --prod"
    }
  ],
  "routes": [
    { "src": "/", "dest": "dist/apps/frontend" },
    { "src": "/page-2", "dest": "dist/apps/frontend/page-2" },
    { "src": "/(.*)", "dest": "dist/apps/frontend/404.html" }
  ]
}





